<%= form_for(@set_group) do |f| %>
  <% if @set_group.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@set_group.errors.count, "error") %> prohibited this set_group from being saved:</h2>

      <ul>
      <% @set_group.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :year %><br>
    <%= f.select(:year, [12,13], { include_blank: false }) %>
  </div>
  <div class="field">
    <%= f.label :subject_id %><br>
    <%= f.select(:subject_id, Subject.all.collect {|p| [ p.name, p.id ] }, { include_blank: false }) %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
